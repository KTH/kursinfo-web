<!DOCTYPE html>
<html lang={{lang}}>

<head>
  <title>KTH | {{title}}</title>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <meta name="description" content="{{description}}">
  <link rel="shortcut icon" href="https://www.kth.se/polopoly_fs/1.871948.1550155901!/favicon.ico">

  {{#if canonicalUrl}}
    <link href="{{canonicalUrl}}" rel="canonical">
  {{/if}}

  <link rel="stylesheet" href="//use.fontawesome.com/releases/v5.7.0/css/all.css"
    integrity="sha384-lZN37f5QGtY3VHgisS14W3ExzMWZxybE1SJSEsQp9S+oqd12jhcu+A56Ebc1zFSJ" crossorigin="anonymous">
  {{prefixStyle '/static/kth-style/css/kth-bootstrap.css' 'styles' 'screen'}}
  {{prefixStyle '/static/app.css' 'styles' 'screen'}}

  {{{render 'styles'}}}

  {{#if blocks.klaroConfig}}
    {{{blocks.klaroConfig}}}
    {{prefixScript '/static/kth-style/js/klaro-no-css.js' 'head-scripts'}}
  {{/if}}
 
  {{prefixScript '/static/vendor.js' 'head-scripts'}}
  {{prefixScript '/static/browserConfig' 'head-scripts'}}
  {{#extend 'head-scripts'}}
    <script src="//www.kth.se/social/toolbar/widget.js"></script>
  {{/extend}}

  {{#if debug}}
    {{prefixScript '/static/lib/firebug-lite/build/firebug-lite.js' 'head-scripts'}}
  {{/if}}

