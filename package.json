{
  "name": "kursinfo-web",
  "version": "2.0.0",
  "description": "Course information page",
  "main": "app.js",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "uri": "git+https://github.com/KTH/kursinfo-web.git"
  },
  "scripts": {
    "build": "bash ./build.sh prod",
    "build-dev": "bash ./build.sh dev",
    "docker:build": "bash ./docker-build-image.sh dev",
    "docker:run": "bash ./docker-run-image.sh dev",
    "test": "NODE_ENV=test jest",
    "test-win": "cross-env NODE_ENV=test jest",
    "test:watch": "jest --watch",
    "test:notify": "jest --watch --notify",
    "docker": "npm install --development && npm run build && npm prune --production",
    "lint": "eslint . && prettier-eslint --list-different **/*.js",
    "postinstall": "npm run mini-css-extract-plugin && bash -c 'rm -rf ./dist; rm -rf ./node_modules/.cache/webpack'",
    "mini-css-extract-plugin": "yarn add mini-css-extract-plugin --no-lockfile --ignore-scripts",
    "prepare": "husky install",
    "start": "bash -c 'cat /KTH_NODEJS; NODE_ENV=production node app.js'",
    "start-dev": "bash -c 'NODE_ENV=development concurrently --kill-others -n build,app \"npm run build-dev\" \"nodemon app.js\"'"
  },
  "dependencies": {
    "@kth/api-call": "^4.0.39",
    "@kth/kth-node-response": "^1.0.7",
    "@kth/kth-node-web-common": "^7.0.12",
    "@kth/log": "^4.0.5",
    "@kth/monitor": "^3.0.25",
    "@kth/server": "^4.0.3",
    "@kth/session": "^3.0.4",
    "applicationinsights": "^2.3.3",
    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "cookie-parser": "^1.4.6",
    "date-fns": "^2.29.2",
    "date-fns-tz": "^1.3.6",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "express-handlebars": "^6.0.6",
    "http-proxy-middleware": "^2.0.6",
    "kth-node-access-log": "^0.2.9",
    "kth-node-configuration": "^2.0.12",
    "kth-node-express-routing": "^2.0.15",
    "kth-node-i18n": "^1.0.14",
    "kth-node-redis": "^3.1.42",
    "kth-style": "^8.0.38",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-router": "^6.4.0",
    "react-router-dom": "^6.3.0",
    "reactstrap": "^9.1.4",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.13",
    "@babel/eslint-parser": "^7.18.9",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@kth/eslint-config-kth": "^3.0.10",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@types/jest": "^28.1.8",
    "ansi-regex": "^6.0.1",
    "babel-jest": "^29.0.1",
    "babel-loader": "^8.2.5",
    "concurrently": "^7.3.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "decache": "^4.6.1",
    "eslint": "^8.22.0",
    "file-loader": "^6.2.0",
    "husky": "^8.0.1",
    "jest": "^29.0.1",
    "jest-environment-jsdom": "29.0.3",
    "jest-extended": "^3.0.2",
    "mini-css-extract-plugin": "^2.6.1",
    "nodemon": "^2.0.19",
    "null-loader": "^4.0.1",
    "path": "^0.12.7",
    "prettier": "^2.6.1",
    "prettier-eslint-cli": "^7.1.0",
    "pretty-quick": "^3.1.3",
    "sass": "^1.54.5",
    "sass-loader": "^13.0.2",
    "set-value": "^4.1.0",
    "terser-webpack-plugin": "^5.3.5",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "engines": {
    "node": "16"
  },
  "jshintConfig": {
    "maxerr": 5
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "printWidth": 120,
    "arrowParens": "avoid"
  }
}
