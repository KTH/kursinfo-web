{
  "name": "kursinfo-web",
  "version": "2.0.0",
  "description": "Course information page",
  "main": "app.js",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "uri": "git+https://github.com/KTH/kursinfo-web.git"
  },
  "scripts": {
    "vendor": "parcel build ./public/js/vendor.js",
    "test": "jest --passWithNoTests",
    "docker": "npm install --development && npm run build && npm prune --production",
    "build": "NODE_ENV=production rm -rf dist && parcel build ./public/js/app/app.jsx && npm run vendor --preserve-comments",
    "build-dev": "cross-env NODE_ENV=development rm -rf dist && parcel build ./public/js/vendor.js --public-url /student/kurser/kurs/static && parcel watch ./public/js/app/app.jsx --public-url /student/kurser/kurs/static",
    "start": "NODE_ENV=production node app.js",
    "start-dev": "cross-env NODE_ENV=development concurrently --kill-others \"npm run build-dev\" \"nodemon --ignore dist/ --ignore public/ app.js\""
  },
  "dependencies": {
    "applicationinsights": "^1.8.7",
    "axios": "^0.20.0",
    "babel-eslint": "^10.0.3",
    "bluebird": "^3.7.2",
    "body-parser": "^1.15.1",
    "co": "^4.6.0",
    "component-registry": "^1.3.1",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.13.4",
    "express-handlebars": "^5.1.0",
    "http-proxy-middleware": "^1.0.5",
    "jest": "^26.4.2",
    "kth-node-access-log": "^0.2.0",
    "kth-node-api-call": "^3.0.11",
    "kth-node-configuration": "^2.0.1",
    "kth-node-express-routing": "^2.0.3",
    "kth-node-i18n": "^1.0.2",
    "kth-node-log": "^3.1.0",
    "kth-node-monitor": "^1.1.6",
    "kth-node-redis": "^3.1.7",
    "kth-node-response": "https://github.com/KTH/kth-node-response#v1.0.0",
    "kth-node-server": "^3.1.2",
    "kth-node-session": "^1.0.4",
    "kth-node-web-common": "^5.1.4",
    "kth-style": "^5.0.18",
    "mobx": "^5.15.7",
    "mobx-react": "^5.4.4",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "reactstrap": "^8.6.0",
    "safe-utils": "^0.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@testing-library/react": "^11.0.4",
    "chai": "^4.0.2",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.0.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.3",
    "eslint-plugin-react-hooks": "^4.0.0",
    "kth-node-build-commons": "^4.1.5",
    "mockery": "^2.0.0",
    "node-mocks-http": "^1.9.0",
    "nodemon": "^2.0.4",
    "parcel-bundler": "^1.12.3",
    "prettier": "^2.1.1",
    "sass": "^1.26.11"
  },
  "engines": {
    "node": "12"
  },
  "jshintConfig": {
    "maxerr": 5
  },
  "prettier": {
    "trailingComma": "none",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "printWidth": 120
  }
}
