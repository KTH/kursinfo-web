{
  "name": "kursinfo-web",
  "version": "2.0.0",
  "description": "Course information page",
  "main": "app.js",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "uri": "git+https://github.com/KTH/kursinfo-web.git"
  },
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "prepare": "husky install",
    "vendor": "parcel build ./public/js/vendor.js",
    "test": "NODE_ENV=test jest",
    "test:watch": "jest --watch",
    "test:notify": "jest --watch --notify",
    "docker": "npm install --development && npm run build && npm prune --production",
    "build": "NODE_ENV=production npm run move-handlebar-pages && rm -rf dist && parcel build ./public/js/app/app.jsx && npm run vendor --preserve-comments",
    "build-dev": "cross-env NODE_ENV=development npm run move-handlebar-pages && rm -rf dist && parcel build ./public/js/vendor.js --public-url /student/kurser/kurs/static && parcel watch ./public/js/app/app.jsx --public-url /student/kurser/kurs/static",
    "build-dev-win": "cross-env NODE_ENV=development npm run move-handlebar-pages-win && rimraf dist && parcel build ./public/js/vendor.js --public-url /student/kurser/kurs/static && parcel watch ./public/js/app/app.jsx --public-url /student/kurser/kurs/static",
    "start": "NODE_ENV=production node app.js",
    "start-dev": "cross-env NODE_ENV=development concurrently --kill-others -n build,app \"npm run build-dev\" \"nodemon -e js,jsx,json,handlebars --ignore dist/ --ignore dist-dev/ app.js\"",
    "start-dev-win": "cross-env NODE_ENV=development concurrently --kill-others -n build,app \"npm run build-dev-win\" \"nodemon -e js,jsx,json,handlebars --ignore dist/ --ignore dist-dev/ app.js\"",
    "move-handlebar-pages": "cp ./node_modules/kth-node-web-common/lib/handlebars/pages/views/error.handlebars ./server/views/system/error.handlebars && cp ./node_modules/kth-node-web-common/lib/handlebars/pages/layouts/errorLayout.handlebars ./server/views/layouts/errorLayout.handlebars",
    "move-handlebar-pages-win": "copy .\\node_modules\\kth-node-web-common\\lib\\handlebars\\pages\\views\\error.handlebars .\\server\\views\\system\\error.handlebars && copy .\\node_modules\\kth-node-web-common\\lib\\handlebars\\pages\\layouts\\errorLayout.handlebars .\\server\\views\\layouts\\errorLayout.handlebars"
  },
  "dependencies": {
    "@kth/kth-node-response": "^1.0.2",
    "@testing-library/jest-dom": "^5.14.1",
    "applicationinsights": "^2.1.3",
    "axios": "^0.21.2",
    "babel-eslint": "^10.0.3",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "co": "^4.6.0",
    "component-registry": "^1.3.1",
    "cookie-parser": "^1.4.5",
    "date-fns": "^2.25.0",
    "date-fns-tz": "^1.1.6",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-handlebars": "^5.3.2",
    "http-proxy-middleware": "^2.0.0",
    "kth-node-access-log": "^0.2.4",
    "kth-node-api-call": "^3.0.16",
    "kth-node-configuration": "^2.0.5",
    "kth-node-express-routing": "^2.0.9",
    "kth-node-i18n": "^1.0.10",
    "kth-node-log": "^3.1.7",
    "kth-node-monitor": "^1.1.12",
    "kth-node-redis": "^3.1.30",
    "kth-node-server": "^3.1.5",
    "kth-node-session": "^1.0.7",
    "kth-node-web-common": "^5.1.16",
    "kth-style": "^6.0.21",
    "mobx": "^5.15.7",
    "mobx-react": "^5.4.4",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-icons": "^4.2.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "reactstrap": "^8.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@kth/eslint-config-kth": "^1.0.10",
    "@testing-library/react": "^12.0.0",
    "babel-jest": "^27.0.5",
    "concurrently": "^6.2.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.29.0",
    "husky": "^6.0.0",
    "jest": "^27.0.5",
    "nodemon": "^2.0.12",
    "parcel-bundler": "^1.12.5",
    "prettier": "^2.3.1",
    "pretty-quick": "^3.1.1",
    "sass": "^1.35.1"
  },
  "resolutions": {
    "node-forge": "0.10.0"
  },
  "engines": {
    "node": "14"
  },
  "jshintConfig": {
    "maxerr": 5
  },
  "prettier": {
    "trailingComma": "none",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "printWidth": 120
  }
}
