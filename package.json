    "kth-node-server": "^3.1.2",
    "kth-node-session": "^1.0.4",
    "kth-node-web-common": "^3.0.6",
    "kth-style": "^5.0.9",
    "kth-style-inferno-bootstrap": "^0.1.25",
    "locale": "^0.1.0",
    "lodash": "^4.17.19",
