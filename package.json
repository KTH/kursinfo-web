{
  "name": "kursinfo-web",
  "version": "2.0.0",
  "description": "Course information page",
  "main": "app.js",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "uri": "git+https://github.com/KTH/kursinfo-web.git"
  },
  "scripts": {
    "build": "bash ./build.sh prod",
    "build-dev": "bash ./build.sh dev",
    "docker:build": "bash ./docker-build-image.sh dev",
    "docker:run": "bash ./docker-run-image.sh dev",
    "test": "NODE_ENV=test jest",
    "test-win": "cross-env NODE_ENV=test jest",
    "test:watch": "jest --watch",
    "test:file:watch": "jest ./server/apiCalls/transformers/offerings-over-several-semesters.test.js --watch",
    "test:notify": "jest --watch --notify",
    "docker": "npm install --development && npm run build && npm prune --production",
    "lint": "eslint . && prettier-eslint --list-different **/*.js",
    "postinstall": "npm run mini-css-extract-plugin && bash -c 'rm -rf ./dist; rm -rf ./node_modules/.cache/webpack'",
    "mini-css-extract-plugin": "yarn add mini-css-extract-plugin --no-lockfile --ignore-scripts",
    "prepare": "husky install",
    "start": "bash -c 'cat /KTH_NODEJS; NODE_ENV=production node app.js'",
    "start-dev": "bash -c 'NODE_ENV=development concurrently --kill-others -n build,app \"npm run build-dev\" \"nodemon app.js\"'"
  },
  "dependencies": {
    "@kth/api-call": "^4.0.40",
    "@kth/kth-node-response": "^1.0.7",
    "@kth/kth-node-web-common": "^8.1.1",
    "@kth/log": "^4.0.7",
    "@kth/monitor": "^4.1.1",
    "@kth/server": "^4.0.4",
    "@kth/session": "^3.0.8",
    "@kth/ug-rest-api-helper": "^1.0.25",
    "applicationinsights": "^2.7.3",
    "axios": "^1.4.0",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^1.3.8",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-handlebars": "^6.0.7",
    "http-proxy-middleware": "^2.0.6",
    "kth-node-access-log": "^0.2.9",
    "kth-node-configuration": "^2.0.13",
    "kth-node-express-routing": "^2.0.16",
    "kth-node-i18n": "^1.0.18",
    "kth-node-redis": "^3.1.47",
    "kth-style": "^8.0.59",
    "react": "^17.0.2",
    "react-data-table-component": "^7.5.3",
    "react-dom": "^17.0.2",
    "react-icons": "^4.10.1",
    "react-router": "^6.15.0",
    "react-router-dom": "^6.15.0",
    "reactstrap": "^8.10.1",
    "rimraf": "^3.0.2",
    "sanitize-html": "^2.11.0",
    "styled-components": "^6.0.7",
    "victory": "^36.6.11",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.10",
    "@babel/core": "^7.22.10",
    "@babel/eslint-parser": "^7.22.10",
    "@babel/plugin-transform-runtime": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-react": "^7.22.5",
    "@faker-js/faker": "^8.2.0",
    "@kth/eslint-config-kth": "^3.0.14",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.4",
    "ansi-regex": "^6.0.1",
    "babel-jest": "^29.6.3",
    "babel-loader": "^9.1.3",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "decache": "^4.6.2",
    "eslint": "^8.47.0",
    "file-loader": "^6.2.0",
    "husky": "^8.0.3",
    "jest": "^29.6.3",
    "jest-environment-jsdom": "29.6.3",
    "jest-extended": "^3.2.4",
    "mini-css-extract-plugin": "^2.7.6",
    "nodemon": "^3.0.1",
    "null-loader": "^4.0.1",
    "path": "^0.12.7",
    "prettier": "^2.8.8",
    "prettier-eslint-cli": "^7.1.0",
    "pretty-quick": "^3.1.3",
    "react-error-boundary": "^3.1.4",
    "sass": "^1.66.1",
    "sass-loader": "^13.3.2",
    "set-value": "^4.1.0",
    "terser-webpack-plugin": "^5.3.9",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "engines": {
    "node": "18"
  },
  "jshintConfig": {
    "maxerr": 5
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "printWidth": 120,
    "arrowParens": "avoid"
  }
}
