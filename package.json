  "engines": {
    "node": "6.9.1"
  },
  "jshintConfig": {
    "maxerr": 5
  },
  "standard": {
    "parser": "babel-eslint",
    "extends": [
      "standard",
      "standard-jsx"
    ],
    "ignore": [
      "public/js/lib"
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      [
        "module-resolver",
        {
          "alias": {
            "^inferno-bootstrap/lib(.+).jsx": "inferno-bootstrap/dist\\1",
            "^kth-style-inferno-bootstrap/lib(.+).jsx": "kth-style-inferno-bootstrap/dist\\1",
            "^inferno-formlib/lib(.+).jsx": "inferno-formlib/dist\\1"
          }
        }
      ]
    ]
  }
}
